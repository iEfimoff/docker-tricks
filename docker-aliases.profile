alias d='docker'
alias db='docker build'
alias dc='docker-compose'
alias dcbuild='dx() { docker-compose -f $1 build; }; dx'
alias dcdown='dx() { docker-compose -f $1 down; }; dx'
alias dcup='dx() { docker-compose -f $1 up -d; }; dx'
alias de='docker exec -it'
alias debash='dx() { docker exec -it $1 bash; }; dx'
alias desqlplus='dx() { docker exec -it $1 sqlplus / as sysdba; }; dx'
alias di='docker images'
alias dlog='docker logs -f'
alias dmount='docker run --rm -v ${PWD}:/d -w /d centos bash'
alias dp='docker stats'
alias dps='docker ps'
alias dpsformat='docker ps --format '\''{{.Names}}'\'' | xargs -I {} echo "echo; echo \"> {}\"; docker port {} | sed '\''s|0.0.0.0|${HOSTNAME}|g'\'';" | (sh; echo)'
alias dr='docker run --rm -it'
alias drbash='dx() { docker run --rm -it $1 bash; }; dx'
alias drestart='docker restart'
alias drm='docker rm -f'
alias dsrestart='sudo service docker restart'
alias dsstart='sudo service docker start'
alias dsstop='sudo service docker stop'
alias dstart='docker start'
alias dstop='docker stop'
